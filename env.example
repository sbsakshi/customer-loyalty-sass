# Meta WhatsApp Cloud API Credentials
# Rename this file to .env and fill in the values

ADMIN_USERNAME=admin
ADMIN_PASSWORD=password
ADMIN_SECRET=your_jwt_secret_key

WHATSAPP_API_TOKEN=your_access_token
WHATSAPP_PHONE_NUMBER_ID=your_phone_number_id

# Redis (Vercel KV) - Auto-populated by Vercel when you create a KV database
# These variables are automatically set in Vercel deployments
# For local development, run: vercel env pull .env.local
KV_URL=
KV_REST_API_URL=
KV_REST_API_TOKEN=
KV_REST_API_READ_ONLY_TOKEN=

# Redis Feature Flags
REDIS_ENABLED=true
REDIS_DEBUG=false
